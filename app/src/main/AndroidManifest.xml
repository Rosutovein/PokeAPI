<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rosutovein.projet3a">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_pokemon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_pokemon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity" android:parentActivityName=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PokedexActivity" android:parentActivityName=".MainActivity" android:label="Pokedex"/>
        <activity android:name=".ToolbarActivity"/>
        <activity android:name=".AboutActivity" android:parentActivityName=".MainActivity" android:label="About me"/>



    </application>

</manifest>